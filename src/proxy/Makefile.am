  ReferenceMapper_i.cpp \
  main.cpp \
  ntsvc.cpp \
  proxy.cpp \
  ReferenceMapper_i.h \
  ntsvc.h \
  proxy.h

../../lib:
	mkdir -p ../../lib


lorica_proxy_DEPENDENCIES = \
 ../../lib

lorica_proxy_LDFLAGS = \
 @ACE_TLS_LDFLAGS@ -L../../lib

lorica_proxy_LDADD = \
