  libTAO_PI.la \
  libTAO_CodecFactory.la \
  libTAO_Valuetype.la \
  libTAO_PortableServer.la \
  libTAO_AnyTypeCode.la \
  libTAO.la \
  $(ACE_BUILDDIR)/ace/libACE.la \
  @ACE_TLS_LIBS@

endif !BUILD_WINCE
